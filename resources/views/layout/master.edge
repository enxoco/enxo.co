        $('#blockInstagram').addClass('active')
        toggleACL('block-socialmedia')
        toggleRestriction('add', 'blockSocialMedia');
      }
      else {
        $('#blockFacebook').removeClass('active')
        $('#blockTwitter').removeClass('active')
        $('#blockSnapchat').removeClass('active')
        $('#blockInstagram').removeClass('active')
        toggleACL('block-socialmedia')
        toggleRestriction('delete', 'blockSocialMedia');
      }
    })
  })
